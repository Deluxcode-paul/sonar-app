e_valid && this.email_valid && this.phone_valid && this.job_title_valid && this.industry_valid && this.current_billing_valid && this.subs_valid) {\n        this.pass = true;\n      } else {\n        this.pass = false;\n      }\n    },\n    subs: function subs() {\n      this.oneonone = this.lead.subs > 999 ? true : false;\n    }\n  },\n  watch: {\n    lead: {\n      handler: function handler(val) {\n        this.first_name_valid = this.lead.first ? true : false;\n        this.last_name_valid = this.lead.last ? true : false;\n        this.email_valid = this.lead.email ? true : false;\n        this.phone_valid = this.lead.phone ? true : false;\n        this.job_title_valid = this.lead.title ? true : false;\n        this.industry_valid = this.lead.industry ? true : false;\n        this.current_billing_valid = this.lead.current ? true : false;\n        this.subs_valid = this.lead.subs > 1 ? true : false;\n        this.oneonone = this.lead.subs > 999 ? true : false;\n        this.verify(val);\n      }\n    },\n    deep: true\n  }\n};",{"version":3,"sources":["bookingForm.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8DA;AACA;AACA;AACA,OAAA,eAAA,MAAA,mBAAA;AAGA,eAAA;AACA,EAAA,IAAA,EAAA,YADA;AAEA,EAAA,UAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,eAAA,EAAA;AAJA,GAFA;AAQA,EAAA,IARA,kBAQA;AACA,WAAA;AACA;AACA,MAAA,IAAA,EAAA,CAFA;AAGA,MAAA,IAAA,EAAA,KAHA;AAKA,MAAA,SAAA,EAAA,CAAA;AACA,gBAAA,OADA;AAEA,uBAAA,kEAFA;AAGA,qBAAA,KAHA;AAIA,qBAAA;AAJA,OAAA,EAMA;AACA,gBAAA,QADA;AAEA,uBAAA,iEAFA;AAGA,qBAAA,IAHA;AAIA,qBAAA;AAJA,OANA,EAYA;AACA,gBAAA,SADA;AAEA,uBAAA,kCAFA;AAGA,qBAAA,KAHA;AAIA,qBAAA;AAJA,OAZA,CALA;AAyBA;AACA,MAAA,IAAA,EAAA,CAAA;AACA,iBAAA,EADA;AAEA,gBAAA,EAFA;AAGA,iBAAA,EAHA;AAIA,iBAAA,EAJA;AAKA,iBAAA,EALA;AAMA,oBAAA,EANA;AAOA,mBAAA,EAPA;AAQA,gBAAA;AARA,OAAA,CA1BA;AAqCA,MAAA,UAAA,EAAA,CAAA,OAAA,EAAA,OAAA,EAAA,cAAA,EAAA,UAAA,EAAA,YAAA,EAAA,iBAAA,EAAA,MAAA,EAAA,MAAA,EACA,UADA,CArCA;AAwCA,MAAA,YAAA,EAAA,CAAA,QAAA,EAAA,MAAA,EAAA,SAAA,EAAA,UAAA,